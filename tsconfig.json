{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2016"],
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "@/data-structure/*": ["data-structure/*"]
    },
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
